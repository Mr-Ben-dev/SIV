# Massa Network Configuration
VITE_MASSA_NETWORK=mainnet

# Massa RPC Endpoints
VITE_MASSA_RPC_HTTP=https://mainnet.massa.net/api/v2
VITE_MASSA_RPC_WS=wss://mainnet.massa.net/api/v2

# Smart Contract Addresses (Replace with your deployed addresses)
VITE_SIV_VAULT_ADDRESS=AS12... # Your SIV Vault contract address
VITE_USDCE_ADDRESS=AS12... # USDC.e token address on Massa mainnet
VITE_WMAS_ADDRESS=AS12... # Wrapped MAS token address
VITE_WETHB_ADDRESS=AS12... # Wrapped ETH.b token address

# DEX Addresses
VITE_DUSA_ROUTER_ADDRESS=AS12... # Dusa DEX router address
VITE_DUSA_QUOTER_ADDRESS=AS12... # Dusa price quoter address
VITE_EAGLEFI_ROUTER_ADDRESS=AS12... # EagleFi router address (fallback)

# Explorer and UI Configuration
VITE_EXPLORER_URL=https://explorer.massa.net
VITE_DEFAULT_SLIPPAGE_BPS=50
VITE_EPOCH_SECONDS=1800
VITE_SLICES_PER_REBALANCE=6

# Optional: Gas and Performance
VITE_DEFAULT_GAS_LIMIT=1000000000
VITE_POLL_INTERVAL_MS=12000
